<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wx.app.mapper.StudentFreeTestMapper">

    <resultMap id="BaseResultMap" type="com.wx.app.entity.StudentFreeTest">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="userId" column="user_id" jdbcType="BIGINT"/>
        <result property="semester" column="semester" jdbcType="VARCHAR"/>
        <result property="reason" column="reason" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,semester,
        reason,is_pass
    </sql>

    <select id="selectFreeList" resultType="com.wx.app.vo.StudentFreeTestVo">
        SELECT
            student_free_test.id,  student_free_test.user_id,  semester, reason,  is_pass,  name,   school_class,  sex,  grade, specialty, college,     specialty_direction
        FROM
            `student_free_test`
        Left JOIN
            student_info
        ON
            student_free_test.user_id = student_info.user_id
    </select>
</mapper>
